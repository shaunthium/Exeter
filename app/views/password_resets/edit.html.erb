    <div class="row jumbotron">
        <div class="col-md-6 col-md-offset-3">
            <%= render 'shared/error_messages', object: @user %>
            <%= render 'layouts/user_fields', view: "reset", url: password_reset_path(params[:id]) %>
        </div>
    </div>
</div>
